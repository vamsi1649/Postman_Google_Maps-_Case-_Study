{
	"info": {
		"_postman_id": "a35e9bec-834f-43cb-815f-82883af0378b",
		"name": "google maps",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
		"_exporter_id": "23215253"
	},
	"item": [
		{
			"name": "add and delete google maps with json",
			"item": [
				{
					"name": "add api for maps",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"const placeid = pm.response.json();\r",
									"pm.environment.set(\"placeid\", placeid.place_id);"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\r\n\r\n    \"location\":{\r\n\r\n        \"lat\" : -38.383494,\r\n\r\n        \"lng\" : 33.427362\r\n\r\n    },\r\n\r\n    \"accuracy\":50,\r\n\r\n    \"name\":\"Frontline house\",\r\n\r\n    \"phone_number\":\"(+91) 983 893 3937\",\r\n\r\n    \"address\" : \"29, side layout, cohen 09\",\r\n\r\n    \"types\": [\"shoe park\",\"shop\"],\r\n\r\n    \"website\" : \"http://google.com\",\r\n\r\n    \"language\" : \"French-IN\"\r\n\r\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{baseurl}}/maps/api/place/add/json?key={{key}}",
							"host": [
								"{{baseurl}}"
							],
							"path": [
								"maps",
								"api",
								"place",
								"add",
								"json"
							],
							"query": [
								{
									"key": "key",
									"value": "{{key}}"
								}
							]
						}
					},
					"response": []
				},
				{
					"name": "delete api for maps",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "DELETE",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\r\n    \"place_id\":\"{{placeid}}\"          \r\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{baseurl}}/maps/api/place/delete/json?key={{key}}",
							"host": [
								"{{baseurl}}"
							],
							"path": [
								"maps",
								"api",
								"place",
								"delete",
								"json"
							],
							"query": [
								{
									"key": "key",
									"value": "{{key}}"
								}
							]
						}
					},
					"response": []
				}
			]
		}
	]
}